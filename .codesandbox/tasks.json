{
  "setupTasks": [
    {
      "name": "Install Dependencies",
      "command": "yarn install"
    }
  ],
  "tasks": {
    "build": {
      "name": "build",
      "command": "yarn build",
      "runAtStart": true
    },
    "start": {
      "name": "start",
      "command": "yarn serve",
      "runAtStart": true,
      "preview": {
        "port": 4173
      }
    },
    "test": {
      "name": "test",
      "command": "yarn test",
      "runAtStart": false
    },
    "eject": {
      "name": "eject",
      "command": "yarn eject",
      "runAtStart": false
    }
  }
}
